include "types.fbs";

namespace ace;

table CacheData {
  u: [uint8];    // uint8   array
  c: [int8];     // int8    array
  s: [string];   // string  array
  i: [int32];    // int32   array
  l: [int64];    // int64   array
  f: [float32];    // float32 array
  size: int64;   // data size
}

table TensorShape {
  dims: [int32];
}

table Tensor {
  name: string; 
  shape: TensorShape;
  valid_shape: TensorShape;
  dtype: DataType = FLOAT_32;
  dformat: DataFormat = NCHW;
  data: CacheData;
  scale: CacheData; 
}


// borrow this code from https://github.com/alibaba/MNN
table ListValue {
    s:[string];
    i:[int];
    f:[float];
    b:[bool];
    type:[DataType];
}

table Attribute {
    s:string;
    i:int = 0;
    b:bool = false;
    key:string(key);
    type:DataType;
    f:float = 0.0;
    tensor:Tensor;
    list:ListValue;
    func:NamedAttrList;
}

table NamedAttrList {
    name: string;
    attr: [Attribute];
}
